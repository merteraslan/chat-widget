{
  "name": "@merteraslan/chat-widget",
  "version": "0.6.3",
  "description": "A modern, lightweight React chat widget with interactive message support and webhook integration.",
  "type": "module",
  "main": "dist/chat-widget.umd.js",
  "module": "dist/chat-widget.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/chat-widget.es.js",
      "require": "./dist/chat-widget.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/merteraslan/chat-widget.git"
  },
  "bugs": {
    "url": "https://github.com/merteraslan/chat-widget/issues"
  },
  "homepage": "https://github.com/merteraslan/chat-widget#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/merteraslan"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "unpkg": "dist/chat-widget.umd.js",
  "jsdelivr": "dist/chat-widget.umd.js",
  "keywords": [
    "ai",
    "chat",
    "widget",
    "webhook",
    "n8n",
    "make.com",
    "zapier",
    "chatwoot",
    "automation",
    "integration",
    "customer-support",
    "conversational",
    "messaging",
    "interactive",
    "chatbot",
    "react",
    "typescript"
  ],
  "author": "Mert Eraslan <mertegithub@gmail.com>",
  "license": "MIT",
  "sideEffects": [
    "**/*.css",
    "./dist/style.css"
  ],
  "scripts": {
  "dev": "vite --open /examples/basic-usage.html",
    "prebuild": "rimraf dist",
  "build": "vite build",
  "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
  "prepublishOnly": "npm run build",
    "publish:npm": "npm publish --access public",
    "pack:check": "npm pack --dry-run",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {}
}
